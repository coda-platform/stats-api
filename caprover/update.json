{
    "appName": "stats-api",
    "instanceCount": 1,
    "notExposeAsWebApp": true,
    "forceSsl": true,
    "envVars": [
        {
            "key": "CODA_STATS_API_PORT",
            "value": "$CODA_STATS_API_PORT"
        },
        {
            "key": "CODA_STATS_API_FAKE_AIDBOX",
            "value": "$CODA_STATS_API_FAKE_AIDBOX"
        },
        {
            "key": "CODA_STATS_API_AIDBOX_URL",
            "value": "$CODA_STATS_API_AIDBOX_URL"
        },
        {
            "key": "CODA_STATS_API_CLIENT_ID",
            "value": "$CODA_STATS_API_CLIENT_ID"
        },
        {
            "key": "CODA_STATS_API_CLIENT_SECRET",
            "value": "$CODA_STATS_API_CLIENT_SECRET"
        }
    ]
}